<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="DragNDropSortListView">
    <attr name="drag_collapsed_height" format="dimension" />
    <attr name="dragNdrop_scroll_start" format="float" />
    <attr name="max_dragNdrop_scroll_speed" format="float" />
    <attr name="dragfloat_background_color" format="color" />
    <attr name="drag_remove_mode">
      <enum name="clickRemove" value="0" />
      <enum name="flingRemove" value="1" />
    </attr>
    <attr name="track_dragNdrop_sort" format="boolean"/>
    <attr name="drag_float_alpha" format="float"/>
    <attr name="drag_slide_shuffle_speed" format="float"/>
    <attr name="drag_remove_animation_duration" format="integer"/>
    <attr name="dragNdrop_animation_duration" format="integer"/>
    <attr name="dragNdrop_enabled" format="boolean" />
    <attr name="dragNsort_enabled" format="boolean" />
    <attr name="drag_remove_enabled" format="boolean" />
    <attr name="dragNdrop_start_mode">
      <enum name="onDown" value="0" />
      <enum name="onMove" value="1" />
      <enum name="onLongPress" value="2"/>
    </attr>
    <attr name="dragNdrop_handle_id" format="integer" />
    <attr name="drag_fling_handle_id" format="integer" />
    <attr name="drag_click_remove_id" format="integer" />
    <attr name="drag_use_default_controller" format="boolean" />
  </declare-styleable>
</resources>
